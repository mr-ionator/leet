class Solution:
    def minBitwiseArray(self, nums: List[int]) -> List[int]:
        ans = []

        for num in nums:
            found = False

            for n in range(num+1):
                if(n | (n+1)) == num:
                    ans.append(n)
                    found = True
                    break
            if not found:
                ans.append(-1)
        return ans